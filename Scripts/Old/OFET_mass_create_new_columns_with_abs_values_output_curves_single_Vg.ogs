// This script is used in output curves taken with the low noise rig in only one gate voltage. It loops over all the workbooks in the folder and creates two new columns, one for the abs(Id) and one for abs(Ig).
[Main]

string sheetname;

doc -ef LB { // loop over all workbooks in folder
	wks.colwidth = 9;// set all col width to 9
	sheetname$=wks.longname$; //get the name of worksheet
	%A=sheetname.GetToken(13,"_")$; // get the string "T" or "O", which shows if the curve is a transfer or output curve
	string trimmedsheetname$=sheetname.GetToken(11,"_")$; // get the 11th string, which shows the elapsed time
	//trimmedsheetname$=Substitute(trimmedsheetname$,,min,0)$; // remove "min" from workbook name
	//trimmedsheetname$=Substitute(trimmedsheetname$,,(newdroplet),0)$; // remove "(newdroplet)" from workbook name
	%K=trimmedsheetname$; // get the string that shows the time
	page.title = 1;  // Let the workbook title shows Long Name only (1 = Long Name, 2 = short name, 3 = both)
	

if(exist(%H,2)==0) //not a workbook, must be a matrix
		continue;
	else 
		if (%A == "T") continue;	// if this is an output curve then break		
			else wks.ncols=6; // set total column number to 6
				// Write long name, units, comments, and parameter1 of column B
				col(E)[L]$ = "Id (Vg=0.0V) /A"; // Long name
				col(E)[U]$ = A; // Units
				col(E)[O]$="abs(col(C))";
				col(F)[L]$ = "Ig (Vg=0.0V) /A"; // Long name
				col(F)[U]$ = A; // Units
				col(F)[O]$="abs(col(D))";
				col(E)[C]$ = "Ultrapure water 32 minutes + phosphate buffer (pH=3.2) %K minutes";
				col(F)[C]$ = col(E)[C]$;
}